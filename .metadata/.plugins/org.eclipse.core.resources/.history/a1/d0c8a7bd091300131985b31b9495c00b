#include <stdio.h>
#include <stdlib.h>

typedef struct treeNode {
	int key;
	struct treeNode* left;
	struct treeNode* right;
} TreeNode;


TreeNode* createTreeNode (int keyVal){
	TreeNode* NewNode = malloc(sizeof(TreeNode));
	NewNode->key = keyVal;
	NewNode->left = NULL;
	NewNode->right = NULL;
	return NewNode;
}

//Takes in an array or numbers and creates a tree from this array
TreeNode* createTree (int* array){


}

TreeNode* insertNode (TreeNode* head, int value){
	TreeNode* origHead = head;

	if (head == NULL){
		head = createTreeNode(value);
		return head;
	}

	else{
		while (head != NULL){
			if (head->key > value){
				head = head->left;
			}
			if (head->key < value){
				head = head->right;
			}
			else{
				printf("Value already exists in tree : %d\n", value);
				return origHead;
			}
		}
	}

	return head;
}

int main (void){

	return 0;
}
